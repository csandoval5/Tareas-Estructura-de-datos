// Tarea Semana 6
//Ejercicio 1  Función que calcule el número de elementos de una lista
using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        List<int> miLista = new List<int> { 1, 2, 3, 4, 5, 6, 7, 8, 9 };

        int numeroDeElementos = miLista.Count();

        Console.WriteLine($"El número de elementos en la lista es: {numeroDeElementos}");
    }
}
